{
    "compilerOptions": {
      "target": "esnext",
      "outDir": "dist",
      "module": "CommonJS",
      "moduleResolution": "node",
      "allowJs": true,
      "checkJs": true,
      "types": ["node"],
      "strict": true,
      "resolveJsonModule": true,
      "noEmitOnError": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["lib/**/*", "webos.d.ts"]
}